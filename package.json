{
  "name": "generate-docker-tags-action",
  "description": "A GitHub action to generate Docker tags.",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "author": "Steven Xie <dev@stevenxie.me",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build --source-map --license licenses.txt",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts && prettier --check **/*.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@typescript-eslint/parser": "^4.6.1",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.12.1",
    "eslint-plugin-github": "^4.1.1",
    "husky": "^4.3.0",
    "prettier": "2.1.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
